<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.k.superbag2">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS"></uses-permission>
    <uses-permission android:name="android.permission.BIND_SCREENING_SERVICE"/>
    <uses-permission android:name="android.permission.GET_TASKS" />
    <application
        android:name="MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ScreenLockActivity"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".activity.EditActivity"></activity>
        <activity android:name=".activity.PreviewActivity"></activity>
        <activity android:name=".activity.NumLockActivity"
            android:launchMode="singleTask">

        </activity>
        <activity android:name=".activity.ChooseLockActivity"></activity>
        <activity android:name=".activity.ChoosePicActivity"></activity>
    </application>

</manifest>